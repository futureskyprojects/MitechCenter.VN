 <!-- ============================================================== -->
<!-- Container fluid  -->
<!-- ============================================================== -->
<div class="container-fluid">
	<!-- ============================================================== -->
	<!-- Start Page Content -->
	<!-- ============================================================== -->
	<div class="card">
		<div class="card-body">
			<h4 class="card-title">Tên của tổ chức hoặc cá nhân sở hữu website này</h4>
			<form method="post">
				<div class="form-group">
					<textarea asp-for="eData" class="form-control" rows="3" placeholder="Nhập tại đây...">@Model.eData</textarea>
					<small id="textHelp" class="form-text text-muted">Tips: Nhập ngắn gọn tên của tổ chức hoặc cá nhân</small>
				</div>
				<button type="submit" class="btn btn-block btn-rounded btn-success">CẬP NHẬT</button>
				@Html.Raw(ViewBag.notification)
			</form>
		</div>
	</div>
	<!-- ============================================================== -->
	<!-- End PAge Content -->
	<!-- ============================================================== -->
	<!-- ============================================================== -->
	<!-- Right sidebar -->
	<!-- ============================================================== -->
